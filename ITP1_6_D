import sys

n, m = map(int, input().split())

A = [[] for i in range(n)]

for i in range(n):
    A[i] = list(map(int, input().split()))

b = [[0]*1 for i in range(m)]

for i in range(m):
    b[i] = int(input())


for row in range(n):
    tmp = 0
    for col in range(m):
        tmp += A[row][col]*b[col]
    print("%d" % (tmp))
